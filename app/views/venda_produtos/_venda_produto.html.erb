<div id="<%= dom_id venda_produto %>">
  <td id="id">
    <%= venda_produto.id %>
  </td>

  <td>
    <%= l(venda_produto.created_at.to_date, format: :default) %>
  </td>
  <td>
    <%= t('valores.peso', value: '%0.3f' %venda_produto.quantidade_total) %>
  </td>

  <td>
    <%= t('valores.moeda', value: '%0.2f' %venda_produto.preco_total) %>
  </td>

  <td>
    <%= venda_produto.forma_pagamento %>
  </td>

</div>
